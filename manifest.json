{
    "manifest_version": 3,
    "name": "Chlee",
    "description": "10duck",
    "version": "1.2.3",
    "permissions": [
        "tabs", "scripting", "background"
    ],
    "host_permissions": [
        "https://www.cheonghyun.com/*"
    ],
    "web_accessible_resources": [{
        "resources": ["video.mp4"],
        "matches": ["https://www.cheonghyun.com/*"]
    }],
    "background": {
        "service_worker": "scripts.js"
    }
}